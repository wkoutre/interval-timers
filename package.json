{
  "name": "workout-app",
  "version": "1.0.0",
  "engines": {
    "node": "8.1.4"
  },
  "description": "A full-featured workout app and custom timer written in React-Redux",
  "main": "./dist",
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.config.js",
    "styles": "./node_modules/.bin/stylus -u autoprefixer-stylus ./src/css/style.styl -o ./src/css/style.css",
    "styles:watch": "./node_modules/.bin/stylus -u autoprefixer-stylus -w ./src/css/style.styl -o ./src/css/style.css",
    "clean-public": "npm run remove-public && mkdir public",
    "remove-public": "node_modules/.bin/rimraf ./public",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-public build:html",
    "build": "babel-node tools/build.js",
    "postbuild": "babel-node tools/publicServer.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wkoutre/react-redux-workout-app.git"
  },
  "keywords": [
    "workout",
    "react",
    "redux"
  ],
  "author": "Nick Koutrelakos",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wkoutre/react-redux-workout-app/issues"
  },
  "homepage": "https://github.com/wkoutre/react-redux-workout-app#readme",
  "dependencies": {
    "@types/lodash": "4.14.50",
    "autoprefixer": "^6.7.7",
    "autoprefixer-stylus": "^0.13.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "6.8.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "bootstrap": "^3.3.7",
    "bootstrap-social": "^5.1.1",
    "cheerio": "^0.20.0",
    "colors": "1.1.2",
    "compression": "^1.6.1",
    "connected-react-router": "^4.2.1",
    "crypto": "0.0.3",
    "css-loader": "^0.28.0",
    "express": "4.13.4",
    "facebook-node": "0.0.3",
    "fb": "^2.0.0",
    "file-loader": "^0.11.1",
    "font-awesome": "^4.7.0",
    "fs": "0.0.1-security",
    "history": "^4.6.1",
    "img-loader": "^2.0.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.17.4",
    "node-geocoder": "^3.16.0",
    "npm-run-all": "^4.0.2",
    "open": "0.0.5",
    "path": "^0.12.7",
    "postcss-loader": "^1.3.3",
    "prop-types": "^15.5.10",
    "re-base": "^2.7.0",
    "react": "^15.5.4",
    "react-bootstrap": "^0.31.0",
    "react-dom": "^15.5.4",
    "react-inlinesvg": "^0.6.0",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^4.0.8",
    "react-svg": "^2.1.19",
    "redux": "^3.6.0",
    "redux-debounce": "^1.0.1",
    "redux-thunk": "^2.2.0",
    "request": "^2.81.0",
    "serve-favicon": "^2.3.0",
    "style-loader": "^0.16.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "svg-injector": "^1.1.3",
    "svg-inline-loader": "^0.7.1",
    "svg-inline-react": "^2.1.0",
    "timer-stopwatch": "^0.2.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-dev-server": "^2.4.5",
    "webpack-hot-middleware": "^2.18.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1"
  }
}
