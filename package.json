  "scripts": {
    "start": "webpack-dev-server",
    "styles": "./node_modules/.bin/stylus -u autoprefixer-stylus ./src/css/style.styl -o ./src/css/style.css",
    "styles:watch": "./node_modules/.bin/stylus -u autoprefixer-stylus -w ./src/css/style.styl -o ./src/css/style.css",
    "build": "webpack --config ./webpack.prod.config.js --progress --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wkoutre/react-redux-workout-app.git"
  },
