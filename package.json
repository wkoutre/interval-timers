    "react",
    "redux"
  ],
  "author": "Nick Koutrelakos",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wkoutre/react-redux-workout-app/issues"
  },
  "homepage": "https://github.com/wkoutre/react-redux-workout-app#readme",
